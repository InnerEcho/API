{
  "name": "innerecho",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"tsx -r tsconfig-paths/register\" src/app.ts",
    "build": "babel src --out-dir dist --extensions \".ts\"",
    "start": "node dist/app.js",
    "db": "node --loader ts-node/esm src/public/insertSpeciesData.ts",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "seed:essential": "sequelize-cli db:seed --seed essential-species-data.cjs",
    "seed:dev": "NODE_ENV=development sequelize-cli db:seed --seed dev-species-data.cjs",
    "seed:dev:undo": "NODE_ENV=development sequelize-cli db:seed:undo --seed dev-species-data.cjs",
    "seed:dev:win": "set NODE_ENV=development && sequelize-cli db:seed --seed dev-species-data.cjs",
    "seed:dev:undo:win": "set NODE_ENV=development && sequelize-cli db:seed:undo --seed dev-species-data.cjs"
  },
  "dependencies": {
    "@google-cloud/speech": "^6.7.0",
    "@langchain/community": "^0.3.11",
    "@langchain/core": "^0.3.16",
    "@langchain/openai": "^0.3.11",
    "@zyphra/client": "^1.0.5",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "debug": "~2.6.9",
    "dotenv": "^16.6.1",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "fluent-ffmpeg": "^2.1.3",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.5",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.3",
    "nodemailer": "^6.9.15",
    "sequelize": "^6.37.4",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.8.5",
    "@types/nodemailer": "^6.4.16",
    "@types/sequelize": "^4.28.20",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.34",
    "babel-plugin-module-resolver": "^5.0.2",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "type": "module"
}
